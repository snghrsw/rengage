{
    "compilerOptions": {
        "lib": [
            "es5",
            "es2015"
        ],
        "module": "commonjs",
        "target": "es5",
		"jsx": "react",
        "outDir": "bin",
        "sourceMap": true,
        "noImplicitAny": false
    },
    "typeRoots": [
      "./node_modules/@types"
    ],
    "exclude": [
        "node_modules",
        "typings"
    ],
    "filesGlob": [
        "./src/server/routes.ts"
    ]
}
