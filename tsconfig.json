{
  "compilerOptions": {
    "lib": [
      "es5",
      "es2015",
      "dom"
    ],
    "allowJs": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": true,
    "jsx": "react",
    "outDir": "bin",
    "sourceMap": true,
    "noImplicitAny": false
  },
  "typeRoots": [
    "./node_modules/@types"
  ],
  "exclude": [
    "node_modules",
    "typings"
  ],
  "filesGlob": [
    "./src/server/routes.ts"
  ]
}
